{
  "manifest_version": 2,

  "name": "Freshservice Ticket Helper",
  "description": "This extension helps technicians use the Freshservice.com web application.",
  "version": "1.1",
  
  "content_scripts": 
  [
    {
      "js": 
      [
        "jquery-3.2.1.min.js",
        "main.js"
      ],
      "css":
      [
        "main.css"
      ],
      "matches": 
      [
        "*://*freshservice.com/helpdesk/*"
        "*://helpdesk.msoe.edu/*"
      ]
    }
  ],
  
  "permissions": ["storage"]
}
